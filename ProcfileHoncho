fastapi: pip install -r fastapi/requirements.txt && uvicorn fastapi/Gender/main:app --host 0.0.0.0 --port $PORT
streamlit: mkdir -p /root/.streamlit && bash -c 'echo -e " [general]\n email = \"\"\n " > /root/.streamlit/credentials.toml' && bash -c 'echo -e " [server]\n enableCORS = false\n port = $PORT\n" > /root/.streamlit/config.toml' && pip install -r streamlit/requirements.txt && streamlit run streamlit/app.py --server.port $PORT